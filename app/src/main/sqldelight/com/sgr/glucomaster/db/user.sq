-- Define table: usuario
CREATE TABLE IF NOT EXISTS usuario (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    email TEXT UNIQUE
);

-- Define query to obtain all users
obtainUsers: SELECT * FROM usuario;

-- Define query to obtain user by email
obtainUser: SELECT * FROM usuario WHERE email = ?;

-- Define query to insert user
insertUser: INSERT INTO usuario (email) VALUES (?);

-- Define query to delete user
deleteUser: DELETE FROM usuario WHERE email = ?;