-- Define table: glucemia
CREATE TABLE IF NOT EXISTS glucemia (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    fecha TEXT NOT NULL,
    turno TEXT NOT NULL,
    valor INTEGER NOT NULL,
    user_id INTEGER NOT NULL,
    FOREIGN KEY (user_id) REFERENCES usuario(id)
);

-- Define a query to insert a glucemia in db
insertGluc: INSERT INTO glucemia (fecha, turno, valor, user_id) VALUES  (?, ?, ?, ?);